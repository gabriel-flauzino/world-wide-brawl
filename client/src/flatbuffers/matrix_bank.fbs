namespace supercell_sfw;

struct FBMatrix2x3 {
    a:float;
    b:float;
    c:float;
    d:float;
    x:float;
    y:float;
}

struct FBShortMatrix2x3 {
    a:short;
    b:short;
    c:short;
    d:short;
    x:short;
    y:short;
}

struct FBColorTransform {
    r:ubyte;
    g:ubyte;
    b:ubyte;
    a:ubyte;
    ra:ubyte;
    ga:ubyte;
    ba:ubyte;
}

table FBMatrixBank {
    matrices:[FBMatrix2x3];
    color_transforms:[FBColorTransform];
    short_matrices:[FBShortMatrix2x3];
}