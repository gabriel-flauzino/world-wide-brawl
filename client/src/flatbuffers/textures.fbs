namespace supercell_sfw;

table FBTexture {
    flags:uint8;
    type:int8;
    width:uint16;
    height:uint16;
    data:[byte];
    texture_file_ref_id:int32;
}

table FBTextureSet {
    lowres_texture:FBTexture;
    highres_texture:FBTexture;
}

table FBTextureSets {
    texture_sets:[FBTextureSet];
}

root_type FBTextureSets;